{
	"name": "TLmanaGer 2",
	"version": "2.0.0",
	"manifest_version": 2,
	"description": "Desarrollamiento tool 2",
	"homepage_url": "https://github.com/joelthorner/TLmanaGer-2",
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": [
			"js/background.js"
		],
		"persistent": false
	},
	"options_page": "src/options/index.html",
	"browser_action": {
		"default_icon": "icons/icon32.png",
		"default_title": "TLmanaGer 2",
		"default_popup": "src/popup/index.html"
	},
	"chrome_url_overrides": {
		"newtab": "src/newTab/index.html"
	},
	"permissions": [
		"chrome://favicon/",
		"clipboardRead",
		"clipboardWrite",
		"contentSettings",
		"contextMenus",
		"cookies",
		"history",
		"bookmarks",
		"management",
		"notifications",
		"tabs",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"js/*"
	],
	"externally_connectable": {
		"matches": [ 
			"*://localhost/*",
			"*://joelthorner.github.io/emilio-generator/"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": [
				"src/inject/inject.css"
			]
		},
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"js/libs/jquery-3.3.1.min.js",
				"js/libs/js.cookie.js",
				"js/actions/load-guide-lines.js"
			]
		}
	]
}